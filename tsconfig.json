{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./src/renderer/dist",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@mui/icons-material": ["node_modules/@mui/icons-material"],
      "@mui/icons-material/*": ["node_modules/@mui/icons-material/*"]
    },
    "lib": ["dom", "es2016"]
  },
  "include": ["src/renderer/react/**/*"]
}